{
  "name": "web-cashback-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "rm -rf build && node ace serve --hmr",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "//comment":"I add this to manage docker commands more easily",
    "docker:dev": "docker compose up -d --build",
    "docker:dev:rebuild": "docker compose down --rmi all && docker compose up -d --build",
    "docker:logs": "docker compose logs -f app",
    "docker:logs:all": "docker compose logs -f",
    "docker:commands": "docker compose exec app node ace $COMMAND"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#tests/*": "./tests/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js",
    "#audit_resolvers/*": "./app/audit_resolvers/*.js",
    "#notifications/*": "./app/notifications/*.js",
    "#jobs/*": "./app/jobs/*.js",
    "#helpers/*": "./app/helpers/*.js",
    "#auth/*": "./app/auth/*.js",
    "#types/*": "./app/types/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.0.0-beta.6",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@japa/api-client": "^2.0.4",
    "@japa/assert": "^4.0.1",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.4",
    "@swc/core": "1.7.26",
